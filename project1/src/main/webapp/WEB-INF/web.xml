<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>Servlets.LoginServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>SubmitReimbursementRequestServlet</servlet-name>
		<servlet-class>Servlets.SubmitReimbursementRequestServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>PendingServlet</servlet-name>
		<display-name>PendingServlet</display-name>
		<description></description>
		<servlet-class>Servlets.PendingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GetPendingReimbursementRequestsServlet</servlet-name>
		<display-name>GetPendingReimbursementRequestsServlet</display-name>
		<description></description>
		<servlet-class>Servlets.GetPendingReimbursementRequestsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateEmployeeInformationServlet</servlet-name>
		<display-name>UpdateEmployeeInformationServlet</display-name>
		<description></description>
		<servlet-class>Servlets.UpdateEmployeeInformationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmployeeInformationServlet</servlet-name>
		<display-name>EmployeeInformationServlet</display-name>
		<description></description>
		<servlet-class>Servlets.EmployeeInformationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewAll</servlet-name>
		<display-name>ViewAll</display-name>
		<description></description>
		<servlet-class>Servlets.ViewAll</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/employeeinformation</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SubmitReimbursementRequestServlet</servlet-name>
		<url-pattern>/submitreimbursementrequestpage.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PendingServlet</servlet-name>
		<url-pattern>/PendingServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateEmployeeInformationServlet</servlet-name>
		<url-pattern>/UpdateEmployeeInformationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>GetPendingReimbursementRequestsServlet</servlet-name>
	<url-pattern>/GetPendingReimbursementRequestsServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeInformationServlet</servlet-name>
		<url-pattern>/EmployeeInformationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewAll</servlet-name>
		<url-pattern>/ViewAll</url-pattern>
	</servlet-mapping>
  
</web-app>